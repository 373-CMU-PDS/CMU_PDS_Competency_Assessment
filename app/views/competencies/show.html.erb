<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @competency.name %>
</p>
  <p>
  <b>Indicators in <%= @competency.name %>:</b>
    <ol>
      <% for indicator in @competency.indicators %>
        <li>
        <%= indicator.description %>
        <% for resource in indicator.indicator_resources %>
        <p>
        <%= resource.name %> 
        <% end %>

        </li>
      <% end %>
    </ol>
  </p>

<%= link_to 'Edit', edit_competency_path(@competency) %> |
<%= link_to 'Back', competencies_path %>