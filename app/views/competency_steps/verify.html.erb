<table class="table table-hover table-expandable" >
  <thead class="pds-color-background-light white-text">
    <tr>
      <th>Competency</th>
      <th>Level</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @competency.indicators.each do |indicator| %>
	    <tr>
	        <td><%= indicator.competency_id %></td>
	        <td><%= indicator.level %></td>
	        <td><%= indicator.description %></td>
	    </tr>
	    <tr>
	  		<td colspan="5">
	  			<h4>Resources</h4>
				<% @competency.indicator_resources.each do |resource| %>
				<ul>
	    			<li><%= resource.resource_id%></li>
	    		</ul>
	    		<% end %>
	    	</td>
	    </tr>
    <% end %>
  </tbody>
</table>