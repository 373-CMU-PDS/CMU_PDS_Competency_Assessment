<h1>Select a Competency</h1>

<!-- Selection Dropdown menu for choosing a Competency -->
<%= form_tag(view_all_level_step_path, method: :get) do %>
  <%= select_tag :competency_id, options_from_collection_for_select(@competencies, 'id', 'name'), {include_blank: 'Select a Competency'} %>
  <%= submit_tag "Search", :name => nil %>
<% end %>







